<!DOCTYPE html>
<html lang="en">
 
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Image Uploading</title>
</head>
 
<body>
    <h1>To Upload Image on mongoDB</h1>
    <hr>
    <div>
        <form action="/gallery" method="POST" enctype="multipart/form-data">
            <div>
                <label for="name">Image Title</label>
                <input type="text" id="name" placeholder="Name"
                       value="" name="title" required>
            </div>
            <div>
                <label for="sub-name">Subtitle</label>
                <input type="text" id="subtitle" placeholder="Subtitle"
                       value="" name="subtitle" required>
            </div>
            <div>
                <label for="image">Upload Image</label>
                <input type="file" id="image"
                       name="file" value="" required>
            </div>
            <div>
                <button type="submit">Submit</button>
            </div>

          </form>
          <!-- <% for (var i = 0; i < data.length; i++) {%>
            <img src="static/images/<%= i.imageUrl %>" alt="background-image" srcset="">
          <% } %> -->

          <% data.forEach(element=> {%>
            <img src="static/images/<%= element.imageUrl %>" alt="background-image" srcset="">
          <% }) %>
            
    </div>
 
    <hr>
</body>
</html>